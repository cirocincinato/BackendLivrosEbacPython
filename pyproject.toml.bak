[tool.poetry]
name = "backend-livros-ebac-python"
version = "0.1.0"
description = "Backend FastAPI do projeto EBAC Python"
authors = [
    { name = "ciro rocha", email = "ciroreipersa@gmail.com" }
]
readme = "README.md"
package-mode = false

[tool.poetry.dependencies]
python = "^3.12"
fastapi = {extras = ["standard"], version = ">=0.115,<1.0"}
sqlalchemy = ">=2.0,<3.0"
aiosqlite = ">=0.19,<0.22"
redis = ">=7.1.0,<8.0.0"
python-dotenv = "*"

[tool.poetry.group.dev.dependencies]
uvicorn = {extras = ["standard"], version = "*"}

[build-system]
requires = ["poetry-core>=2.0.0,<3.0.0"]
build-backend = "poetry.core.masonry.api"
